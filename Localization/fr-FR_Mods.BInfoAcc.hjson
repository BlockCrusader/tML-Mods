Configs: {
	ConfigServer: {
		DisplayName: Configuration Principale

		easySell: {
			Label: Marchand vend les nouveaux accessoires d'information
			Tooltip:
				'''
				Quand mis à vrai, le Marchand vend les accessoires d'information de ce mod selon la phase de la lune
				Bon si vous voulez une façon facile d'obtenir les nouveaux affichages d'information
				'''
		}

		extendedRecipe: {
			Label: Ajouter les nouveaux accessoires à la recette du PDA
			Tooltip:
				'''
				Bascule si la reccette du PDA est écrasée de façon à exiger le Miroir de Divination, le Compteur C.R.M., et l'Analyseur de Menaces de ce mod
				Si vous ne voulez pas vous taper plus de travail pour avoir votre cellulaire, essayez de désactiver ça
				'''
		}

		updatedPhones: {
			Label: Activer les nouveaux affichages d'inforation sur le PDA, le Téléphone Portable, et le Téléphone Coquillage
			Tooltip:
				'''
				Détermine si le PDA et ses améliorations héritent des nouveaux affichages d'information de ce mod
				Prend également en charge le Wishing Glass du mod Thorium
				'''
		}

		refreshRate: {
			Label: Taux de Mise à Jour des Informations
			Tooltip:
				'''
				Certains affichages d'information (Taux/Limite d'Apparition et Biome) ne sont pas mis à jour à chaque image
				Cela empêche du texte scintillant, et réduit le potentiel de création de lag
				Le délai entre les mises à jour peut être personnalisé ici; la valeur établie est la quantité de ticks entre chaque mise à jour
				Pour référence, 1 seconde = 60 ticks
				'''
		}

		cycleRate: {
			Label: Fréquence de Cycle d'Affichage d'Information
			Tooltip:
				'''
				Certains affichages d'information (Taux/Limite d'Apparition, Combo, et Biome) ont plusieurs lignes d'information qu'il montrent en cycle
				Ce comportement est similaire à celui de la Radio du Temps vanille pendant qu'une tempête de sable est active
				Ce paramètre multiplie la vitesse à laquelle les différentes lignes d'information cyclent
				'''
		}

		prioModBiomes: {
			Label: L'affichage de Biomes Priorise les Biomes de Mods
			Tooltip:
				'''
				Fait en sorte que l'affichage de biomes affiche les biomes de mods supportés avant les mods vanille (pour la plupart)
				Tous les biomes environnants font quand-même partie du cycle; les biomes à plus haute priorité sont affichés plus tôt dans le cycle
				L'affichage de biomes supporte les mods ci-bas:
				'Thorium Mod', 'Calamity Mod', 'Mod of Redemption', 'The Stars Above', 'Spirit Mod', 'Spooky Mod', 'Confection Rebaked', 
				'The Depths', 'Aequus', 'Verdant', 'Food, Furniture & Fun', and 'Remnants'
				'''
		}
	}
}
