Items: {
	EngiRegistry: {
		DisplayName: Registre de l'Ingénieur
		Tooltip:
			'''
			Affiche la quantité d'Emplacements de Sentinelle que vous avez et combien sont occupés
			'Tu t'approches dan-génieur-eusement de ta limite de sentinelles'
			'''
	}

	AttendanceLog: {
		DisplayName: Journal de Présences
		Tooltip: Affiche la quantité d'Emplacements de Sbires que vous avez et combien sont occupés
	}

	FortuneMirror: {
		DisplayName: Miroir de Fortune
		Tooltip: Affiche la chance
	}

	HitMarker: {
		DisplayName: Marqueur de Coup
		Tooltip:
			'''
			Affiche le nombre de coups, la durée, et le total de dommage des combos
			Un combo consiste des coups frappés consécutivement à moins de 3 secondes l'un de l'autre
			'''
	}

	RSH: {
		DisplayName: Compteur C.R.M.
		Tooltip: Affiche le combo, la regénération de vie, et l'économie de mana
	}

	SafteyScanner: {
		DisplayName: Scanneur de Sécurité
		Tooltip: Affiche le taux d'apparition des ennemis
	}

	ScryingMirror: {
		DisplayName: Miroir de Divination
		Tooltip: Affiche la chance, les emplacements de sbires, et les emplacements de sentinelles
	}

	SmartHeart: {
		DisplayName: Cœur Intelligent
		Tooltip:
			'''
			Affiche la régénération et/ou la perte de vie
			'Assure-toi de faire tes pas'
			'''
	}

	BiomeCrystal: {
		DisplayName: Cristal de Biome
		Tooltip: Affiche le(s) biome(s) dans lequel vous vous situez
	}

	Magimeter: {
		DisplayName: Magimètre
		Tooltip: Affiche la régénération de mana, ou l'utilisation quand un item consommant la mana est utilisé
	}

	WantedPoster: {
		DisplayName: Avis de Recherche
		Tooltip:
			'''
			Affiche l'aggro, qui détermine la probabilité que les ennemies vous ciblent
			'Ils ne se sont même pas souciés de la partie "ou vivant"...'
			'''
	}

	ThreatAnalyzer: {
		DisplayName: Analyseur de Menaces
		Tooltip: Affiche le taux d'apparition, l'aggro, et le(s) biome(s)
	}
}

