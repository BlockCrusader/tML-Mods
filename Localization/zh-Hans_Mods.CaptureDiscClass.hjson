Configs: {
	ClientConfig: {
		DisplayName: 用户设置

		autoDeployDiscs: {
			Label: 默认自动使用
			Tooltip:
				'''
				让捕猎碟默认为自动使用模式
				进入世界时，这个功能通常是关闭的，需要手动打开
				自动使用模式可以让你手持捕猎碟即可使用，不需要按住鼠标
				适合觉得按住鼠标画圈很麻烦的人 (例如画圈时误触UI导致攻击中断)
				要停止使用捕猎碟,需要使用热键切换物品栏 (滚轮在正在使用物品时无效)!
				你也可以通过热键进行自动使用模式的开关(请在控件菜单里绑定按键!)
				'''
		}

		rangerMode: {
			Label: 巡护员模式
			Tooltip:
				'''
				让游玩体验更接近MOD的灵感来源：《宝可梦巡护员》系列;
				- 轨迹中断: 捕猎碟接触敌怪时轨迹会被重置
				- 脆弱捕猎碟: 捕猎碟接触敌怪或敌对射弹时，捕猎碟的轨迹会被重置。此外,捕猎碟受到攻击会损毁，需要重新使用。
				- 共享伤害: 与脆弱捕猎碟类似, 但敌对射弹击中你的捕猎碟或捕猎碟轨迹,或是敌怪接触到捕猎碟时，你会受到伤害!!!
				只是为了玩起来更像《宝可梦巡护员》，并没有对应的奖励。
				'''
		}

		trueRangerMode: {
			Label: 真巡护员模式
			Tooltip:
				'''
				在巡护员模式中选择'共享伤害' 才会正常生效
				共享伤害还会造成以下影响;
				- 大多数治疗方式(比如治疗药水)没有效果
				- 只有捕猎碟与鞭子以外的召唤武器可以造成伤害.
				- 使用捕猎碟时,玩家本体不会受伤(但会通过捕猎碟受伤)
				- 战斗中生命恢复降低，使用捕猎碟会阻止生命自然恢复
				- 圈入敌怪时暂时允许生命恢复. 用捕获伤害击杀敌怪时效果更高
				为了玩起来更像《宝可梦巡护员》而设计的受伤方式
				弱化生命恢复是为了阻止玩家利用无敌时间拖延来使用药水
				同样，命中再生是为了鼓励持续攻击的风险回报
				警告: 此模式没有特殊奖励，纯粹为了特殊挑战而设计，请不要指望平衡性
				'''
		}

		damageTimeScaling: {
			Label: 随画图时间增加伤害
			Tooltip:
				'''
				激活时，将根据画出封闭图形所花的时间按比例增加伤害
				更长时间画出的图形最高可以造成3倍伤害，使得快速画出图形效率更低
				你的DPS与手速关系更小，防止你为了打出伤害损伤你的手
				注意，这并不是预期的游戏方式，会影响平衡性
				总体而言，此设置会降低游玩的劳累程度，但对平衡有负面影响
				这个设定与MOD的一些武器与饰品类似，可以与之叠加
				'''
		}

		Headers: {
			// EaseOfUse: 简化操作
			// RangerMode: 巡护员模式
			// Balancing: 平衡性调整
		}

		captureSpdMult: {
			// Label: 捕猎碟速度倍数
			/* Tooltip:
				'''
				用于调整捕获攻击速度
				捕猎碟的速度决定了它跟随光标的速度，进而影响手感
				更快的捕猎碟可以更好的反应操作，这个选项可以提升所有捕猎碟的手感
				对于早期的捕猎碟更明显，这些捕猎碟被设计得操控性较差
				类似 '随画图时间增加伤害'选项，这并不是预期的游戏方式，会影响平衡性
				如果通过触屏或数位板游玩，强烈建议调整，因为捕猎碟对它们的反应很差
				''' */
		}

		captureDmgMult: {
			// Label: 捕猎碟伤害倍数
			/* Tooltip:
				'''
				对所有捕获伤害生效
				方便觉得捕猎碟过强或过弱的人调整平衡
				请注意，捕猎碟的对单DPS较差
				这是对其强大的对群控制力和整个屏幕的攻击范围的平衡
				''' */
		}

		rangerModeDmg: {
			// Label: 巡护员模式伤害倍数
			/* Tooltip:
				'''
				和 '捕猎碟伤害倍数'一样, 但只于巡护员模式生效
				两个选项的倍数可以叠加
				建议使用，因为在巡护员模式下圈入目标要困难得多
				''' */
		}

		rangerModeBossDmg: {
			// Label: 巡护员模式Boss伤害
			/* Tooltip:
				'''
				在巡护员模式下对BOSS造成的额外伤害倍数 
				能与其他伤害增益叠加
				建议使用，在巡护员模式下，BOSS的体型和攻击性使得圈入BOSS要困难得多
				''' */
		}

		rangerModeSpd: {
			// Label: 巡护员模式速度倍数
			/* Tooltip:
				'''
				和 '捕猎碟速度倍数'一样, 但只于巡护员模式生效
				两个选项的倍数可以叠加
				建议使用，因为在巡护员模式下圈入目标要困难得多
				''' */
		}
	}
}

